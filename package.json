  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@nuxt/test-utils": "^0.1.3",
    "@nuxt/types": "^2.15.2",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
