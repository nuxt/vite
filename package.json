{
  "name": "nuxt-vite",
  "version": "0.2.4",
  "description": "Vite Experience with Nuxt 2",
  "repository": "nuxt/vite",
  "license": "MIT",
  "exports": {
    ".": "./dist/nuxt-vite.js",
    "./dist/runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/nuxt-vite.js",
  "types": "./dist/nuxt-vite.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build",
    "prepublishOnly": "yarn build",
    "dev": "yarn fixture:dev",
    "fixture:dev": "nuxt dev test/fixture",
    "fixture:build": "nuxt build test/fixture",
    "fixture:generate": "nuxt generate test/fixture",
    "fixture:start": "nuxt start test/fixture",
    "lint": "eslint --ext .ts .",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && yarn jest"
  },
  "dependencies": {
    "@vitejs/plugin-legacy": "^1.5.3",
    "consola": "^2.15.3",
    "fs-extra": "^10.0.0",
    "postcss-import": "^14.0.2",
    "postcss-import-resolver": "^2.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.3",
    "semver": "^7.3.5",
    "ufo": "^0.7.9",
    "upath": "^2.0.1",
    "vite": "^2.5.4",
    "vite-plugin-vue2": "^1.8.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/composition-api": "^0.28.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "eslint": "^7.32.0",
    "jest": "^27.1.0",
    "mkdist": "^0.3.3",
    "nuxt": "^2.15.8",
    "playwright": "^1.14.1",
    "sass": "^1.39.0",
    "siroc": "^0.16.0",
    "standard-version": "^9.3.1"
  }
}
